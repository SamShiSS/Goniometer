% first I will show you what each vibration feels like
fourmotors('E',' ',1,1); % single stimulus

fourmotors('E','F',2,1); % two simultaneous stimuli

fourmotors('E','F',3,1); % two sequential stimuli

fourmotors('F',' ',1,1);

% ask subject whether it's clear
% repeat above upon subject's request
% now respond to vibration using push button

% Test whether tactor A, B, C, D works
fourmotors('A','',1,1);
fourmotors('B','',1,1);
fourmotors('C','',1,1);
fourmotors('D','',1,1);
